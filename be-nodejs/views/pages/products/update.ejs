        <div class="main-panel">
          <div class="content-wrapper ">
            <div class="page-header">
              <h3 class="page-title"> Update Product </h3>
            </div>
            <div class="row">
              <div class="col-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <form class="forms-sample" method="POST" action="/api/v1/admin-page/product/<%= product._id %>?_method=PUT" >
                      <div class="form-group">
                        <label for="exampleInputName1">Name</label>
                        <input type="text" class="form-control" id="exampleInputName1" name="name" value="<%= product.name %>" placeholder="Name">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail2">Color</label>
                        <input type="text" class="form-control" id="exampleInputName1" name="color" value="<%= product.color %>" placeholder="Color">

                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail3">Version</label>
                        <input type="text" class="form-control" id="exampleInputEmail3" name="version" value="<%= product.version %>" placeholder="Version">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail4">Price</label>
                        <input type="text" class="form-control" id="exampleInputEmail4" name="price" value="<%= product.price %>" placeholder="Price">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail5">Discount</label>
                        <input type="text" class="form-control" id="exampleInputEmail5" name="discount" value="<%= product.discount %>" placeholder="Discount">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail5">Amount</label>
                        <input type="text" class="form-control" id="exampleInputEmail5" name="amount" value="<%= product.amount %>" placeholder="Amount">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail5">Description</label>
                        <input type="text" class="form-control" id="exampleInputEmail5" name="descripton" value="<%= product.description %>" placeholder="Description">
                      </div>
                      <div class="form-group">
                        <label for="content">Content</label>
                        <input type="text" class="form-control" id="editor" name="content"  placeholder="Content" value="<%= product.content %>">
                      </div>
